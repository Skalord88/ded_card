<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.7"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.7
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.7.xsd">

  <changeSet id="4" author="oscar">
      <createTable tableName="character_ability">
        <column name="id" type="int" autoIncrement="true">
          <constraints primaryKey="true"/>
        </column>
        <column name="streght" type="int">
          <constraints nullable="false"/>
        </column>
        <column name="dextrity" type="int">
          <constraints nullable="false"/>
        </column>
        <column name="constitution" type="int">
          <constraints nullable="false"/>
        </column>
        <column name="intelligence" type="int">
          <constraints nullable="false"/>
        </column>
        <column name="wisdom" type="int">
          <constraints nullable="false"/>
        </column>
        <column name="charisma" type="int">
          <constraints nullable="false"/>
        </column>
      </createTable>
  </changeSet>
</databaseChangeLog>