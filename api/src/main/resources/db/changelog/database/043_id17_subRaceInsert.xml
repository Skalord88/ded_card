<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.7"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.7 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.7.xsd">
    <changeSet id="17" author="damian">
        <insert tableName="sub_race">
            <column name="sub_race_name" value="Lightfoot" />
            <column name="race_id" value="1" />
            <column name="size_id" value="4" />
            <column name="modifiers" value="[
            {
            &quot;modifier&quot; : &quot;SPEED&quot;
            &quot;bonus&quot; : 20;
            &quot;targets&quot; : []
            }
            ]" />
            <column name="level_adjustment" value="0" />
        </insert>
    </changeSet>
</databaseChangeLog>

